.feed-link {
  margin-top: 22px;

  a {
    display: inline-block;
    padding: 3px 15px;
    color: rgb(252, 100, 35);
    border: 0px solid rgb(252, 100, 35);
    border-radius: 4px;

    i {
      color: rgb(252, 100, 35);
      font-size: 12px;
    }

    &:hover {
      color:white;
      background: rgb(252, 100, 35);

      i { color: white; }
    }
  }
}
